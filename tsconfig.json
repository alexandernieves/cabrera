{
  "compilerOptions": {
  
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "build"
  ],
  
  "extends": "expo/tsconfig.base"
}
